{
    "name": "stijink/homescreen",
    "description": "Personal smart mirror project",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "johngrogg/ics-parser": "^2.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^4.2",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.2",
        "symfony/lock": "^4.2",
        "symfony/monolog-bundle": "^3.2",
        "symfony/yaml": "^4.1",
        "zendframework/zend-feed": "^2.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "php-vcr/php-vcr": "^1.4",
        "php-vcr/phpunit-testlistener-vcr": "^3.2",
        "phpstan/phpstan": "^0.11.1",
        "phpunit/phpunit": "^7.3",
        "symfony/browser-kit": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/var-dumper": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C2BPPB7BT194247EJKJ2WTXM",
            "allow-contrib": false
        }
    }
}
